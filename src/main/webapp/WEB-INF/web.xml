<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="4.0"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
   http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <display-name>Archetype Created Web Application</display-name>


    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>UsersShowServlet</servlet-name>
        <servlet-class>ru.mail.ilya6697089.app.controller.UsersShowServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsersShowServlet</servlet-name>
        <url-pattern>/showUsers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UsersAddServlet</servlet-name>
        <servlet-class>ru.mail.ilya6697089.app.controller.UsersAddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsersAddServlet</servlet-name>
        <url-pattern>/addUsers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserDeleteServlet</servlet-name>
        <servlet-class>ru.mail.ilya6697089.app.controller.UserDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserDeleteServlet</servlet-name>
        <url-pattern>/users/delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MenuServlet</servlet-name>
        <servlet-class>ru.mail.ilya6697089.app.controller.MenuServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MenuServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

</web-app>

